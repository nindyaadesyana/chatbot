{
  "name": "frontend-tvku",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npx tsx scripts/checkDocker.ts",
    "dev": "next dev --turbopack",
    "dev:only": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ingest": "npx tsx scripts/ingest.ts",
    "ingest:enhanced": "npx tsx scripts/enhancedIngest.ts",
    "health": "npx tsx scripts/healthCheck.ts",
    "services": "npx tsx scripts/startServices.ts",
    "docker:up": "docker-compose up -d chromadb",
    "docker:down": "docker-compose down",
    "docker:restart": "npx tsx scripts/dockerManager.ts restart",
    "docker:status": "npx tsx scripts/dockerManager.ts status",
    "docker:cleanup": "npx tsx scripts/dockerManager.ts cleanup",
    "docker:ingest": "docker-compose --profile ingest up auto-ingest",
    "docker:full": "docker-compose up -d chromadb && docker-compose --profile ingest up auto-ingest",
    "dev:full": "npm run services && npm run dev",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "chroma:monitor": "npx tsx scripts/keepChromaAlive.ts",
    "dev:monitored": "npm run chroma:monitor & npm run dev"
  },
